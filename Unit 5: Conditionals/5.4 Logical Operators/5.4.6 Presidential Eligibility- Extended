user_age = int(input("What is your age?: "))
country_of_origin = input("Born in the U.S.? (Yes/No): ")
years_of_residency = int(input("Years of Residency: "))

if((user_age >= 35) and (country_of_origin == "Yes") and (years_of_residency >= 14)):
    print("You are eligible to run for president.")
else:
    print("You are not eligible to run for president.")
    if (user_age < 35):
        print("You are too young to run for president.")
    if (country_of_origin != "Yes"):
        print("You must be born in the U.S. to run for president.")
    if (years_of_residency < 14):
        print("You have not been a resident for long enough.")
